    <script>
            const choices = new Choices('[data-trigger]',
            {
                searchEnabled: false,
                itemSelectText: '',
            });

            function DownloadExcelInsurance(){
                window.open("/Insurance/ExcelExportInsurance");
            }
            function filterSort(filterSortingId){
            var filterData = { FilterSort : document.getElementById(filterSortingId).classList[0] , SearchWord : ""  };
            $.ajax({
            type: "POST",
            url: "/Insurance/FilterSorting/",
            data: filterData,
            //dataType: "json",
            success: async function (result) {
                    if(result.includes('Desc')){
                        document.getElementById(result.replace('Desc','')).classList.remove(result);
                        document.getElementById(result.replace('Desc','')).classList.add(result.replace('Desc',''));
                    }
                    else{
                        document.getElementById(result).classList.remove(result);
                        document.getElementById(result).classList.add(result+"Desc");
                    }
                    //alert("Aycana aşığım");
                    }
                });
            }
            function GetUserInformation(){
                let userIdentity =  document.getElementById("identificationNumber").value;
                debugger;
                        $.ajax({
                    type: "POST",
                    url: "/Mernis/GetUserInformation/",
                    data: {userIdentity},
                    dataType: "text",
                    success:  function (user) {
                        user = JSON.parse(user);
                        if(user.message == undefined){
                        document.getElementById("name").value = user.name;
                        document.getElementById("lastName").value = user.lastName;
                        document.getElementById("birthDate").value = user.birthYear;
                        }
                        else{
                            alert(user.message);
                        }
                            }
                        });
            }
            function GetInsuranceInformation(insuranceId){
                            $.ajax({
            type: "POST",
            url: "/Insurance/GetInsuranceInformation/",
            data: {insuranceId:insuranceId},
            dataType: "json",
            success:  function (insurance) {
